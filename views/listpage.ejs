<!DOCTYPE html>
<html>
<head> <% include ./head %> </head>
<body>
<% include ./header %>

    <a class="btn" href="/additem">Add New Item</a>

    <div class="card yellow">

    <table class="highlight">
        <tr>
            <th>addedBy</th>
            <th>url</th>
            <th>VideoGameName</th>
            <th>createdby</th>
        </tr>

        <% items.forEach(function(item){  %>

        <tr>
            <td> <%= item.addedBy%></td>
            <td><a href="<%= item.url%>" target="_blank">Link to localhost7000</a></td>
            <td> <%= item.videoGameName%></td>
            <td> <%= item.createdby%></td>
            <td><a href="/delete?videoGameName=<%=item.videoGameName%>">Delete</a></td>

        </tr>



        <% }) %>
    </table>

    </div>

<% include ./footer %>
</body>
</html>
